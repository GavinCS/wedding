<div class="row accommodation">
  <div class="col-sm-6">
    <ul>
      <li><strong>Name: </strong><%= @accommodation.name %></li>
      <li><strong>Website: </strong><%= @accommodation.website %></li>
      <li><strong>Email: </strong><%= @accommodation.email %></li>
      <li><strong>Phone: </strong><%= @accommodation.contact_number %></li>
      <li><strong>Address:</strong> <%= @accommodation.address %></li>
    </ul>
  </div>
  <div class="col-sm-6">
      <div id="map" class="acc-map-single" data-marker-image='<%= @image %>' data-marker='<%=raw @marker.to_json %>'></div>
  </div>
</div>


<p>
  <%= link_to "Edit", edit_admin_accommodation_path(@accommodation) %> |
  <%= link_to "Destroy", admin_accommodation_path(@accommodation), :confirm => 'Are you sure?', :method => :delete %> |
  <%= link_to "View All", admin_accommodations_path %>
</p>
